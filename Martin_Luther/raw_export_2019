        <text>Still Image</text>
      </Type>
      <Publisher>
        <text>[Johann Grünenberg</text>
      </Publisher>
      <Identifier>
        <text>4585559</text>
      </Identifier>
      <Language>
        <text>Latin</text>
      </Language>
    </DublinCore>
